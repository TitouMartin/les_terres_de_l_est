<ul>
  <% @albums.each do |album| %>
    <li><%= album.name %></li>
    <ul>
      <% album.album_tracks.each do |track| %>
        <li><%= track.name %></li>
      <% end %>
    </ul>

    <ul>
      <% album_artist.each do |artist| %>
        <li><%= artist.name %></li>
      <% end %>
    </ul>

  <% end %>
</ul>

